{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' https://js.stripe.com; frame-src https://js.stripe.com https://hooks.stripe.com;"
        }
        ]
      }
    ]
  }
  